<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">	<display-name>Basic WebApp</display-name>
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>net.spikesync.basic.webapp.UserServlet</servlet-class>
	</servlet>
	<!--  element servlet-mapping is already superseded by the annotation @WebServlet("/userservlet/*")  -->
		<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/UserServletDD/*</url-pattern>
	</servlet-mapping>
</web-app>
